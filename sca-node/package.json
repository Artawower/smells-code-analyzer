{
  "name": "smells-code-analyzer",
  "version": "0.1.2",
  "description": "CLI tool powered by LSP and tree-sitter for finding dead and smells code from your project",
  "main": "dist/index.js",
  "files": [
    "dist/**",
    "bin/sca"
  ],
  "type": "module",
  "bin": {
    "sca": "bin/sca"
  },
  "exports": {
    ".": "./dist/*"
  },
  "scripts": {
    "dev": "ts-node --esm ./src/index.ts",
    "build": "tsc --project tsconfig.json && chmod +x ./bin/sca",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artawower/smells-code-analyzer.git"
  },
  "keywords": [
    "cli",
    "code-analyzer",
    "tree-sitter",
    "lsp"
  ],
  "author": "darkawower <artawower@protonmail.com> (https://du-blog.ru/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Artawower/smells-code-analyzer/issues"
  },
  "homepage": "https://github.com/Artawower/smells-code-analyzer#readme",
  "devDependencies": {
    "@types/node": "24.7.2",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsm": "2.3.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@vscode/ripgrep": "1.15.14",
    "commander": "14.0.1",
    "ripgrep-node": "1.0.0",
    "ripgrep-wrapper": "1.1.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-typescript": "0.23.2",
    "ts-lsp-client": "1.1.0",
    "walk-sync": "4.0.1"
  },
  "trustedDependencies": [
    "@vscode/ripgrep",
    "esbuild",
    "tree-sitter",
    "tree-sitter-javascript",
    "tree-sitter-typescript"
  ]
}
